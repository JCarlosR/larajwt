{
  "name": "JCarlosR/larajwt",
  "description": "JWT authentication tools and guard for Laravel projects",
  "keywords": [
    "JWT",
    "Laravel",
    "Guard",
    "Authentication",
    "API",
    "REST"
  ],
  "homepage": "https://github.com/JCarlosR/larajwt",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Milad Rahimi",
      "email": "info@miladrahimi.com",
      "homepage": "http://miladrahimi.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "info@miladrahimi.com",
    "source": "https://github.com/JCarlosR/larajwt/issues"
  },
  "require": {
    "php": ">=7.0",
    "lcobucci/jwt": "^3.2",
    "laravel/framework": "^5.5|^6|^7|^8"
  },
  "autoload": {
    "psr-4": {
      "MiladRahimi\\LaraJwt\\": "src/MiladRahimi/LaraJwt/",
      "MiladRahimi\\LaraJwtTests\\": "tests/"
    }
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^3.5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MiladRahimi\\LaraJwt\\Providers\\ServiceProvider"
      ],
      "aliases": {
        "JwtAuth": "MiladRahimi\\LaraJwt\\Facades\\JwtAuth"
      }
    }
  }
}
